n = input()
while n >= 0 {
    if n > 1 {
        print(0)
        if n == 3 {
            print(0)
        }
        else {
            print(1)
        }
    }
    else {
        print(1)
        if n > 0 {
            print(0)
        }
        else {
            print(1)
        }
    }
    n = n - 1
}