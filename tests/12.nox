n = read()
while n >= 0 {
    if n > 1 {
        write(0)
        if n == 3 {
            write(0)
        }
        else {
            write(1)
        }
    }
    else {
        write(1)
        if n > 0 {
            write(0)
        }
        else {
            write(1)
        }
    }
    n = n - 1
}